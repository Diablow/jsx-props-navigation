{
	"name": "jsx-props-navigation",
	"author": {
		"name": "Luis Silva",
		"email": "luis.silva@medigranja.com"
	},
	"publisher": "diablow",
	"displayName": "jsx.props.navigation",
	"description": "Easy Props/attributes navigation",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jsxPropsNavigation.moveRight",
		"onCommand:jsxPropsNavigation.addRight",
		"onCommand:jsxPropsNavigation.moveLeft",
		"onCommand:jsxPropsNavigation.addLeft"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings":[
			{
				"command": "jsxPropsNavigation.moveRight",
				"key": "alt+win+]",
				"mac": "alt+cmd+]",
				"when": "editorTextFocus"
			},
			{
				"command": "jsxPropsNavigation.addRight",
				"key": "ctrl+alt+win+]",
				"mac": "ctrl+alt+cmd+]",
				"when": "editorTextFocus"
			},
			{
				"command": "jsxPropsNavigation.moveLeft",
				"key": "alt+win+[",
				"mac": "alt+cmd+[",
				"when": "editorTextFocus"
			},
			{
				"command": "jsxPropsNavigation.addLeft",
				"key": "ctrl+alt+win+[",
				"mac": "ctrl+alt+cmd+[",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
